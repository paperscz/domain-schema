{
  "name": "@domain-schema/formik",
  "version": "0.0.34",
  "description": "React Forms generator for Domain Schema",
  "repository": "https://github.com/sysgears/domain-schema.git",
  "author": "Victor Vlasenko <victor.vlasenko@sysgears.com>",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json -w",
    "lint": "tslint -p tsconfig.json --fix -c ../../tslint.json",
    "tests": "jest",
    "test": "yarn tests && yarn lint",
    "snap": "jest -u",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "lib/*.map"
  ],
  "devDependencies": {
    "@domain-schema/core": ">= 0.0.30",
    "@domain-schema/validation": ">= 0.0.30",
    "@types/react": "^16.0.39",
    "@types/react-dom": "^16.0.4",
    "bootstrap": "^4.3.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "reactstrap": "^6.0.1",
    "react-dom": "^16.2.0"
  },
  "peerDependencies": {
    "@domain-schema/core": ">= 0.0.30",
    "@domain-schema/validation": ">= 0.0.30",
    "prop-types": ">= 15.6.0",
    "react": ">= 16.2.0",
    "react-dom": ">= 16.2.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "humps": "^2.0.1",
    "formik": "^0.11.11"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
